// constant
const MOD1: usize = 1_000_000_007;
const MOD9: usize = 998_244_353;
const INF: usize = 1001001001001001001;

fn main() {
    input! {
        N: usize,
        A: [usize; N],
    }

    // dp[i] := iマス目を黒く塗る場合の数
    let mut dp = vec![Mod998::new(0); N];
    dp[0] = 1.into();

    for i in 0..N {
        for j in 1.. {
            if i + A[i] * j < N {
                let tmp = dp[i];
                dp[i + A[i] * j] += tmp;
            } else {
                break;
            }
        }
    }

    debug!(dp);

    let ans = dp.into_iter().sum::<Mod998>();
    println!("{}", ans);
}

