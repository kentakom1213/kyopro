// attributes
#![allow(unused_imports)]
#![allow(unused_variables)]
#![allow(non_snake_case)]
#![allow(dead_code)]
#![allow(unused_macros)]

// imports
use itertools::Itertools;
use proconio::{
    input,
    marker::{Bytes, Chars, Usize1},
};

macro_rules! debug {
    ( $($val:expr),* $(,)* ) => {{
        #[cfg(debug_assertions)]
        eprintln!( concat!($(stringify!($val), " = {:?}, "),*), $($val),* );
    }};
}

fn main() {
    input! {
        N: usize,
        A: [usize; N]
    }
    // 平方分割
    const SIZE: usize = 200;

    // dp[i] :=
    let mut dp = vec![Mod998::new(0); N];
    dp[0] = 1.into();

    // dp[i][j] := iで割ったあまりがjである場合
    let mut buckets = vec![vec![Mod998::new(0); SIZE]; SIZE];

    for (i, &a) in (0..).zip(&A) {
        // 自分の値を取得
        for d in 1..SIZE {
            dp[i] += buckets[d][i % d];
        }
        // 加算
        if a < SIZE {
            // aが小さいとき→バケットに加算
            buckets[a][i % a] += dp[i];
        } else {
            // aが大きいとき→直接可算
            for j in 1.. {
                if i + a * j >= N {
                    break;
                }
                let tmp = dp[i];
                dp[i + a * j] += tmp;
            }
        }
    }

    let ans = dp.into_iter().sum::<Mod998>();
    println!("{ans}");
}

